server {
    listen 443 ssl;
    ssl_certificate /etc/nginx/key-crt/server.crt;
    ssl_certificate_key /etc/nginx/key-crt/server.key;
    ssl_client_certificate /etc/nginx/key-crt/client.crt;
    ssl_verify_client on;
    #ssl_client_certificate /etc/nginx/key-crt/ca.crt;
    #ssl_verify_client optional;
    location / {
        proxy_pass http://127.0.0.1:8265;
    }
}
